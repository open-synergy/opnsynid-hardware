<openerp>
    <data>
        <record id="base_print_policy_view_form" model="ir.ui.view">
            <field name="name">base.print.policy.proxy.form</field>
            <field name="model">base.print.policy</field>
            <field name="inherit_id" ref="base_print_policy.base_print_policy_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='download_group_ids']" position="after">
                    <field name="proxy_group_ids" widget="many2many_tags"/>
                    <field name="escpos_group_ids" widget="many2many_tags"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
